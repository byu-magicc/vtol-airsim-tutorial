    "latex-workshop.chktex.args.root": [
        "-wall",
        "-n1",
        "-n6",
        "-n16",
        "-n18",
        "-n21",
        "-n22",
        "-n30",
        "-n44",
        "-n46",
        "-q"
    ],
    "latex-workshop.chktex.args.active": [
        "-wall",
        "-n1",
        "-n6",
        "-n9",
        "-n13",
        "-n16",
        "-n17",
        "-n18",
        "-n21",
        "-n22",
        "-n26",
        "-n30",
        "-n44",
        "-n45",
        "-n46",
        "-q"
    ],
    "latex-workshop.message.latexlog.exclude": [
        "Unknown document class"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-shell-escape",
                "-f",
                "%DOC%"
            ],
            "env": {}
        },
    ],
    "latex-workshop.latex.recipe.default": "latexmk",
    "latex-workshop.latex.verbatimEnvs": [
        "verbatim",
        "lstlisting",
        "minttcb",
        "minted",
        "codeinline"
    ],
    "outline.showConstants": true,
    "outline.showVariables": true,
    "outline.showModules": true,
    "outline.showBooleans": true,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "latex-workshop.intellisense.triggers.latex": [
        "{", ":"
    ],
    "latex-workshop.intellisense.useTabStops.enabled": true,
    "editor.wordWrapColumn": 110,
    "editor.rulers": [110],
    "editor.wrappingIndent": "indent",
    "editor.wrappingStrategy": "advanced",
    "editor.autoClosingOvertype": "always",
    "[bibtex]": {
        "editor.wordWrap": "off",
    },
}